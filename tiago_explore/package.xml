<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>tiago_explore</name>
  <version>0.0.0</version>
  <description>This package was implemented as part of the Udacity C++ Nanodegree Capstone project. The package provides a node structure to control the tiago robot simulation in an arbitrary world. </description>
  <maintainer email="dominik.schmoeller@gmail.com">dschmoeller</maintainer>
  <license>Apache License 2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>sensor_msgs</depend>
  <depend>std_msgs</depend>
  <depend>custom_interfaces</depend>
  <depend>gemetry_msgs</depend>



  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
